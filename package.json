{
  "name": "borgjs",
  "version": "2.0.1",
  "description": "ðŸ“¦  A tiny wrapper for BorgBackup to automate your backup workflow",
  "main": "dist",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "borgjs": "bin/index.js"
  },
  "files": [
    "bin",
    "lib",
    "dist"
  ],
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf dist && mkdir dist",
    "prepublish": "npm run lint && npm run test && npm run build",
    "build": "npm run clean && babel lib -d dist",
    "test": "tape  -r babel-register test/**/*spec.js | tap-spec",
    "lint": "standard",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "format": "prettier --write --single-quote 'lib/**/*.js' && standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vesparny/borgjs.git"
  },
  "keywords": [
    "borg",
    "backup",
    "attic",
    "cron",
    "crontab"
  ],
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (http://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/borgjs/issues"
  },
  "homepage": "https://github.com/vesparny/borgjs#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "dateformat": "^2.0.0",
    "execa": "^0.6.3",
    "filenamify": "^2.0.0",
    "meow": "^3.7.0",
    "node-notifier": "^5.1.2",
    "nodemailer": "^3.1.8",
    "os-tmpdir": "^1.0.2",
    "proper-lockfile": "^2.0.0",
    "request": "^2.79.0",
    "semver": "^5.3.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^2.0.0",
    "prettier": "^0.22.0",
    "rimraf": "^2.5.4",
    "standard": "^9.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog/"
    }
  }
}
