{
  "name": "borgjs",
  "version": "1.1.1",
  "description": "ðŸ“¦  A tiny wrapper for BorgBackup to automate your backup workflow",
  "main": "dist",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "borgjs": "bin/index.js"
  },
  "files": [
    "bin",
    "lib",
    "dist"
  ],
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf dist && mkdir dist",
    "prepublish": "npm run lint && npm run test && npm run build",
    "build": "npm run clean && babel lib -d dist",
    "test": "tape  -r babel-register test/**/*spec.js | tap-spec",
    "lint": "standard",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vesparny/borgjs.git"
  },
  "keywords": [
    "borg",
    "backup",
    "attic",
    "cron",
    "crontab"
  ],
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (http://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/borgjs/issues"
  },
  "homepage": "https://github.com/vesparny/borgjs#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "dateformat": "^1.0.12",
    "execa": "^0.5.0",
    "filenamify": "^1.2.1",
    "meow": "^3.7.0",
    "node-notifier": "^4.6.1",
    "nodemailer": "^2.6.4",
    "os-tmpdir": "^1.0.2",
    "proper-lockfile": "^2.0.0",
    "request": "^2.75.0",
    "run-parallel": "^1.1.6",
    "semver": "^5.3.0",
    "update-notifier": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "rimraf": "^2.5.4",
    "standard": "^8.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog/"
    }
  }
}
